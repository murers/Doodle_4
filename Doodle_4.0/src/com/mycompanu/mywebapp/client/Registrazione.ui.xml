<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
		<g:AbsolutePanel height="300px">
			<g:at top="10" left="10">
				<g:TextBox text="Nome" readOnly="true"/>
			</g:at>
			<g:at top="50" left="10">
				<g:TextBox text="Username" readOnly="true"/>
			</g:at>
			<g:at top="90" left="10">
				<g:TextBox text="Email" readOnly="true"/>
			</g:at>
			<g:at top="130" left="10">
				<g:TextBox text="Password" readOnly="true"/>
			</g:at>
			<g:at top="10" left="189">
				<g:TextBox name="campo_nome" ui:field="textbox_nome"/>
			</g:at>
			<g:at top="50" left="189">
				<g:TextBox ui:field="textbox_username"/>
			</g:at>
			<g:at top="90" left="189">
				<g:TextBox name="campo_email" ui:field="textbox_email"/>
			</g:at>
			<g:at top="130" left="189">
				<g:TextBox name="campo_password" ui:field="textbox_password"/>
			</g:at>
			<g:at top="170" left="10">
				<g:Button text="Registrazione" width="173px" height="30px" ui:field="registrazione"/>
			</g:at>
			<g:at top="170" left="189">
				<g:Button text="Annulla" width="173px" height="30px" ui:field="annulla"/>
			</g:at>
		</g:AbsolutePanel>
	</g:HTMLPanel>
</ui:UiBinder> 